        $('.js-reset-mailing-filter').on('click', function () {
            $('.mailing-filter-checkbox,.gender-input').prop('checked', false);
            $('.js-mail-filter-search-city ').val('');
        });

        $('.js-open-items-mailing-profiles').on('click',function(){
            $('.new-mailing-profiles-line,.js-mailing-archive-item').toggleClass('open');
        });

        $('.js-edit-mailing-objectives').on("click", function () {
           $(".js-edit-mailing-objectives-form").slideToggle();
        });

        // mailing partially hidden
        $(function () {
            var list = $('.js-mailing-list-profiles-row li');

            for(i = 0; i < list.length; i ++){
                var zIndex = 100;
                $(list[i]).css({'z-index' : + zIndex - i });
            }
        });

        $(function () {
            var list = $('.js-mailing-black-list-users-row li');

            for(i = 0; i < list.length; i ++){
                var zIndex = 100;
                $(list[i]).css({'z-index' : + zIndex - i });
            }
        });



        // mailing checkboxes black list
        $('.js-selection-all-profile-for-black-list').on('click', function() {
            var mailingListCheckbox = $('.js-black-list-checkbox');

            if(mailingListCheckbox.filter(':checked').length === mailingListCheckbox.length){
                mailingListCheckbox.prop('checked', false);
            }else{
                mailingListCheckbox.prop('checked', true);
            }
        });


        $('.new-female-profile-rules-slider').owlCarousel({
            items: 1,
            loop: true,
            nav: true,
            dots: true,
            dotsEach: true,
            navText: ['<i class="icomoon icomoon-chevron-left">\n' + ' \n' + ' </i>' , '<i class="icomoon icomoon-chevron-right">\n' + ' \n' + ' </i>']
        });

        $(function(){
            $('.new-profile__popup-reg-slider').owlCarousel({
                items: 4,
                loop: true,
                nav: true,
                dots: true,
                dotsEach: true,
                navText: ['<i class="icomoon icomoon-chevron-left">\n' + ' \n' + ' </i>' , '<i class="icomoon icomoon-chevron-right">\n' + ' \n' + ' </i>']
            });
        });


        // Hide all popups then show needle popup
        $('.js-popup-view-button-12').on('click', function () {
            var dataPopup = $(this).attr('data-call-popup');
            var overlay = $('.overlay');

            $(dataPopup).show();
            $(overlay).show();

        });

        $('.js-new-profile-close-this-popup-button').on('click',function () {
            var dataPopup = $(this).parent();
            var overlay = $('.overlay');

            $(dataPopup).hide();
            $(overlay).hide();
            $('.new-mailing-overlay').hide();
        });

        // mailing popups

        $('.js-view-mailing-btn').on("click", function (e) {
            var dataPopup = $(this).attr('data-call-popup');
            var overlay = $('.new-mailing-overlay');

            $(dataPopup).show();
            $(overlay).show();
        });
        $('.mailing-popup').on("click", function (e) {
            e.stopPropagation()
        });
        $('.new-mailing-overlay').on("click", function () {
            $(this).children().children().hide();
            $(this).hide();
        });

        // checkbox
        var checkboxFunction = function () {
            var albumCheckboxes = $('input[type="checkbox"].js-new-profile-album-checkbox'),
                profileAlbumWrapper = $('.js-new-profile__album'),
                resetCheckbox = $('.js-new-profile__album-reset-checkbox');

            albumCheckboxes.on('change', function () {
                if (albumCheckboxes.filter(':checked').length > 0) {
                    profileAlbumWrapper.addClass('new-profile__album-delete-mode');
                } else {
                    profileAlbumWrapper.removeClass('new-profile__album-delete-mode');
                }
            });

            // reset checkbox function
            resetCheckbox.on('click', function() {
                albumCheckboxes.prop('checked', false);
                profileAlbumWrapper.removeClass('new-profile__album-delete-mode');
            });
        };

        checkboxFunction();
    });

</script>


