<script type="text/javascript">
    $(document).ready(function () {
        var ajaxFunction = function (data) {
            var results = [];

            $.each(data.response, function (i, val) {
                results.push({value: val.id, text: val.title});
            });

            return results;
        };

        var options = {
            url: function (phrase) {
                return "/api/location/search?title=" + phrase;
            },

            listLocation: "response",
            getValue: "title",
            requestDelay: 100,
            minCharNumber: 1,
            list: {
                maxNumberOfElements: 5,
                onChooseEvent: function (event) {
                    $('.city-search').each(function (key, value) {
                        var $this = $(value);

                        $this.parent().parent().find('.hidden-city-search').val($this.getSelectedItemData().id);
                    });
                },
                onLoadEvent: function (event) {
                    $('.city-search').each(function (key, value) {
                        var $this = $(value);

                        if ($this.getItems().length == 1) {
                            $this.parent().parent().find('.hidden-city-search').val($this.getItems()[0].id);
                        }
                    });
                }
            },

            template: {
                type: "custom",
                method: function (value, item) {
                    var cityName, countryName = '';

                    if (item.city) {
                        cityName = item.city;
                    } else {
                        cityName = item.country;
                    }

                    if (item.region) {
                        countryName += item.region;
                    }

                    if (item.country) {
                        countryName += countryName == '' ? item.country : ', ' + item.country;
                    }

                    return '<p class="--new-select__city-name">' + cityName + '</p><p class="citysearch__new-select__country-name">' + countryName + '</p>';
                }
            }
        };

        var reg_options = $.extend({}, options);

        reg_options['url'] = function (phrase) {
            return "/api/location/search?registration=true&title=" + phrase;
        };

        $('.city-search').each(function (key, value) {
            var $element = $(value);

            if ($element.hasClass('registration-city-search')) {
                $([$.makeArray($element)]).easyAutocomplete(reg_options);
            } else {
                $([$.makeArray($element)]).easyAutocomplete(options);
            }
        });

        $('.picnic_carousel').picnic();

        $(".chosen-select").chosen({no_results_text: "Ничего не найдено по запросу"});

        $(document).tooltip();

        $('#client_city_chosen input, #search_city_chosen input').attr('placeholder', 'Введите название города...');
    });
</script>
